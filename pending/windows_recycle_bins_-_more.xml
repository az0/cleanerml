    <action command="delete" search="glob" path="O:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="P:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="Q:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="R:\$Recycle.Bin"/>
