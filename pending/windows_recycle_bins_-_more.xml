<cleaner id="windows_recycle_bins_more" os="windows">
  <label translate="true">Windows Recycle Bins - More</label>
  <description>Deletes the Windows Recycle Bins of all drives from C to Z</description>
  <option id="windows_recycle_bins_all">
    <label>Windows Recycle Bins - Delete all</label>
    <description>Deletes the Windows Recycle Bins of all drives from C to Z</description>
    <warning>Use this cleaner only if you know what you do!</warning>
    <action command="delete" search="walk.all" path="C:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="D:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="E:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="F:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="G:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="H:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="I:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="J:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="K:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="L:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="M:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="N:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="O:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="P:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="Q:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="R:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="S:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="T:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="U:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="V:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="W:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="X:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="Y:\$Recycle.Bin"/>
    <action command="delete" search="walk.all" path="Z:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="C:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="D:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="E:\$Recycle.Bin"/>
    <action command="delete" search="glob" path="F:\$Recycle.Bin"/>
