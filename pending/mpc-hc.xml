<cleaner id="mpc-hc" os="windows">
  <label>MPC-HC/MPC-BE (Media Player Classic)</label>
  <description>Media player</description>
  <!-- That we don't need so much translation, we combine some options in "History"! Tobias. -->
  <option id="history">
    <label>History</label>
    <description>Delete the usage history</description>
  <!-- That we don't need so much translation, we combine some options in "History"! Tobias.
  <option id="mru">
    <label>Most recently used (MRU)</label>
    <description>Delete the most recently used (MRU) list</description>
  -->
    <action command="delete" search="file" path="%AppData%\MPC-HC\default.mpcpl"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Recent Dub List"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Recent File List"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 0"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 1"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 2"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 3"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 4"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 5"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 6"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 7"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 8"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 9"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 10"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 11"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 12"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 13"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 14"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 15"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 16"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 17"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 18"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Name 19"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="LastOpenDir"/>
    <action command="winreg" path="HKCU\Software\MPC-BE\Recent File List"/>
  </option>
  <!-- That we don't need so much translation, we combine some options in "History"! Tobias.
  <option id="file-positions">
    <label>File positions</label>
    <description>Delete the file positions</description>
  -->
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 0"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 1"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 2"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 3"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 4"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 5"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 6"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 7"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 8"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 9"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 10"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 11"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 12"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 13"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 14"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 15"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 16"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 17"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 18"/>
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="File Position 19"/>
  <!-- That we don't need so much translation, we combine some options in "History"! Tobias.
  </option>
  <option id="dvd-positions">
    <label>DVD positions</label>
    <description>Delete the DVD positions</description>
  -->
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 0"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 1"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 2"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 3"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 4"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 5"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 6"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 7"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 8"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 9"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 10"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 11"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 12"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 13"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 14"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 15"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 16"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 17"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 18"/>
    <action command="winreg" path="HKCU\Software\Gabest\Media Player Classic\Settings" name="DVD Position 19"/>
  <!-- That we don't need so much translation, we combine some options in "History"! Tobias.
  </option>
  <option id="snapshotpath">
    <label>Snapshot Path</label>
    <description>Delete the data</description>
  -->
    <action command="winreg" path="HKCU\Software\MPC-HC\MPC-HC\Settings" name="SnapshotPath"/>
  </option>
  <option id="dmp-files">
    <label>DMP files</label>
    <description>Delete the files</description>
    <action command="delete" search="glob" path="%AppData%\Media Player Classic\*.dmp"/>
    <action command="delete" search="glob" path="%AppData%\MPC-*\*.dmp"/>
  </option>
</cleaner>
