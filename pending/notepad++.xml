<cleaner id="notepad++" os="windows">
  <label>Notepad++</label>
  <description>Editor</description>
  <option id="backup">
    <label>Backup files</label>
    <description>Delete the backup files</description>
    <action command="delete" search="walk.files" path="$APPDATA\Notepad++\backup"/>
  </option>
</cleaner>
