<cleaner id="netbeans_ide" os="windows">
  <label>NetBeans IDE</label>
  <description>Integrated development environment (IDE)</description>
  <running type="exe">netbeans.exe</running>
  <running type="exe">netbeans64.exe</running>
  <option id="logs">
    <label>Logs</label>
    <description>Delete the logs (keep latest log)</description>
    <!-- This application used in software development so deleting all logs is a bad idea -->
    <action command="delete" search="walk.all" path="$APPDATA\NetBeans\8.*\var\log\" regex="\w+(\.\d+)$"/>
  </option>
  <option id="cache">
    <label>Cache</label>
    <description>
Delete the cache.
Cache will be regenerated on next application startup. Reduces cache size by removing outdated files.</description>
    <action search="walk.all" command="delete" path="$LOCALAPPDATA\NetBeans\Cache"/>
  </option>
</cleaner>
