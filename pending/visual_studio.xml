<cleaner id="visual_studio" os="windows">
  <label>Visual Studio</label>
  <description>Integrated Development Environment (IDE)</description>
  <option id="logs">
    <label>Logs</label>
    <description>Delete the logs</description>
    <action command="delete" search="walk.files" path="%LocalAppData%\Microsoft\VisualStudio\*\*.LOG*"/>
    <action command="delete" search="file" path="%AppData%\Microsoft\VisualStudio\*\ActivityLog.Setup.xml"/>
    <action command="delete" search="file" path="%AppData%\Microsoft\VisualStudio\*\ActivityLog.xsl"/>
    <action command="delete" search="walk.files" path="%AppData%\Microsoft Visual Studio\logs\"/>
  </option>
  <option id="temp">
    <label>Temporary files</label>
    <description>Delete the temporary files</description>
    <action command="delete" search="walk.files" path="%TMP%\dd_bootstrapper_*"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_client_*"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_service_*"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_setup_*"/>
    <action command="delete" search="file" path="%TMP%\dd_SetupUtility.txt"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_vs_*"/>
    <!-- .NETFramework: -->
    <action command="delete" search="walk.files" path="%TMP%\.NETFramework*"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_dotNetFx*"/>
    <action command="delete" search="walk.all" path="%TMP%\eraserInstallBootstrapper\"/>
    <action command="delete" search="glob" path="%TMP%\eraserInstallBootstrapper\"/>
    <action command="delete" search="walk.files" path="%TMP%\dd_wcf_CA_smci_*"/>
    <action command="delete" search="walk.files" path="%TMP%\Microsoft .NET Framework *"/>
    <!-- Visual C++ Redist: -->
    <action command="delete" search="walk.files" path="%TMP%\dd_vcredist*"/>
    <action command="delete" search="walk.all" path="%WinDir%\Temp\logishrd\"/>
    <action command="delete" search="glob" path="%WinDir%\Temp\logishrd\"/>
    <action command="delete" search="walk.files" path="%TMP%\Microsoft.VC*.manifest"/>
  </option>
  <option id="backup">
    <label>Backup files</label>
    <description>Delete the backup files</description>
    <action command="delete" search="walk.files" path="%ProgramData%\Microsoft Visual Studio\MachineStorage.dat.bak"/>
  </option>
  <option id="cache">
    <label>Cache</label>
    <description>Delete the cache</description>
    <action command="delete" search="walk.files" path="%LocalAppData%\Microsoft\VisualStudio\*\ComponentModelCache\"/>
    <action command="delete" search="walk.files" path="%LocalAppData%\Microsoft\VisualStudio\*\Extensions\extensions.*.cache"/>
    <action command="delete" search="walk.files" path="%LocalAppData%\Microsoft\VisualStudio\*\Extensions\extensionSdks.*.cache"/>
    <action command="delete" search="file" path="%LocalAppData%\Microsoft\VisualStudio\*\ImageLibrary\ImageLibrary.cache"/>
    <action command="delete" search="walk.files" path="%AppData%\Visual Studio Setup\GPUCache\"/>
  </option>
</cleaner>
