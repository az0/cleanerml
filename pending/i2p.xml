<cleaner id="i2p">
  <label>I2P</label>
  <description>I2P</description>
  <option id="logs">
    <label>Logs</label>
    <description>Delete the logs</description>
    <action command="delete" search="glob" path="%AppData%\I2P\logs\*"/>
    <action command="delete" search="file" path="%AppData%\I2P\eventlog.txt"/>
    <action command="delete" search="file" path="%AppData%\I2P\wrapper.log"/>
    <action command="delete" search="file" path="%AppData%\I2P\addressbook\log.txt"/>
  </option>
  <option id="temp_files">
    <label>Temp Files</label>
    <description>Remove the Temp Files</description>
    <action command="delete" search="glob" path="%LocalAppData%\Temp\i2p-*"/>
  </option>
</cleaner>
