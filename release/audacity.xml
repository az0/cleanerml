<cleaner id="audacity" os="windows">
  <label>Audacity</label>
  <description>Audio editor</description>
  <option id="autosave">
    <label>AutoSave files</label>
    <description>Deletes autosave files.</description>
    <action command="delete" search="walk.files" path="$APPDATA\Audacity\AutoSave\*.autosave"/>
  </option>
</cleaner>
