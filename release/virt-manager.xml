<cleaner id="virt-manager" os="linux">
  <label>Virt-Manager</label>
  <description>Xen/KVM Virtualization Managment GUI</description>
  <option id="logs">
    <label>Logs</label>
    <description>Delete the logs</description>
    <action command="delete" search="file" path="~/.virt-manager/virt-manager.log"/>
  </option>
</cleaner>
