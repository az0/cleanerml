<cleaner id="sketchupmake" os="windows">
  <label>SketchUp Make</label>
  <description>3D modelling editor</description>
  <option id="recent">
    <label>Recent file list</label>
    <description>Deletes recent files entries from File menu.</description>
    <action command="winreg" path="HKCU\Software\SketchUp\SketchUp 2013\Recent File List"/>
  </option>
  <option id="autosavefile">
    <label>Auto-save file</label>
    <description>Deletes file link.</description>
    <action command="winreg" path="HKCU\Software\SketchUp\SketchUp 2013\AutoSave" name="AutoSaveFile"/>
  </option>
</cleaner>
