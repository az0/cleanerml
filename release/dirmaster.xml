<cleaner id="dirmaster" os="windows">
  <label>DirMaster</label>
  <description>Commodore disk image manager</description>
  <option id="dirmru">
    <label>Most recently used disk image search locations</label>
    <description>Deletes recently used folder paths from Find options menu.</description>
    <action command="winreg" path="HKCU\Software\Style\DirMaster\DirMRU"/>
  </option>
  <option id="filemru">
    <label>Most recently used disk image files</label>
    <description>Deletes recent disk image entries from File menu.</description>
    <action command="winreg" path="HKCU\Software\Style\DirMaster\FileMRU"/>
  </option>
</cleaner>
