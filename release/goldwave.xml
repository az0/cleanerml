<cleaner id="goldwave" os="windows">
  <label>Goldwave</label>
  <description>Audio editor</description>
  <option id="mru">
    <label>Most recently used</label>
    <description>Deletes recent files entries from File menu.</description>
    <action command="winreg" path="HKCU\Software\GoldWave\GoldWave\Recent Files"/>
  </option>
</cleaner>
